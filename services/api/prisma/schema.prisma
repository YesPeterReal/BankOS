datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Plugin {
  id          String    @id @default(uuid())
  name        String
  description String
  version     String
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}